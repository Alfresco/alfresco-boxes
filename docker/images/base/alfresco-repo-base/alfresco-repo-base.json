    "builders": [
        {
            "type": "docker",
            "image": "maoo/alfresco-web-base:latest",
            "export_path": "alfresco-repo-base.tar"
        }
    ],
    "provisioners": [
        {
            "cookbook_paths": [
                "../../../../common/vendor-cookbooks"
            ],
            "data_bags_path": "../../../../common/data_bags",
