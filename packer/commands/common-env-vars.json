[
  ["LC_ALL","en_US.UTF-8"],
  ["LANG","en_US.UTF-8"],
  ["GIT_PREFIX","git@github.com"],
  ["GIT_ACCOUNT_NAME","`echo ${GIT_REPO%????} | cut -d \"/\" -f 4`"],
  ["CURRENT_VERSION","`cat metadata.rb| grep version|awk '{print $2}' | tr -d \\\"`"],
  ["INCREMENTED_VERSION","`echo $CURRENT_VERSION | awk -F'[.]' '{print $1 \".\" $2 \".\" $3+1}'`"],
  ["MVN_REPO_CREDS_ID","alfresco-internal"],
  ["MVN_REPO_ID","internal-releases"],
  ["MVN_REPO_HOST","https://artifacts.alfresco.com/nexus"],
  ["MVN_REPO_URL","$MVN_REPO_HOST/content/repositories/$MVN_REPO_ID"],
  ["GROUP_ID","org.alfresco.devops"],
  ["MVN_INTERNAL_REPO_URL","$MVN_REPO_URL/content/repositories/$MVN_REPO_ID"],
  ["ARTIFACT_ID","=`echo ${GIT_REPO%????} | cut -d \"/\" -f 5`"],
  ["CURRENT_VERSION_SUFFIX","-SNAPSHOT"],
  ["CURRENT_VERSION","${CURRENT_VERSION}${CURRENT_VERSION_SUFFIX}"],
  ["GIT_PROJECT_NAME","$ARTIFACT_ID"],
  ["DATABAGS_ARTIFACTID","alfresco-databags"],
  ["DATABAGS_GROUPID","org.alfresco.devops"],
  ["DATABAGS_VERSION","0.2.7"],
  ["DATABAGS_PATH","~/.alfresco-spk/vagrant/packer/data_bags.tar.gz"],
  ["DATABAGS_FOLDER_PATH","~/.alfresco-spk/vagrant/packer/data_bags"],
  ["KS_FILE_URL","https://raw.githubusercontent.com/Alfresco/alfresco-spk/master/ks/ks-centos.cfg"],
  ["BERKS_COOKBOOK_URL","file://$PWD/berks-cookbooks.tar.gz"]
]
